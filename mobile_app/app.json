{
  "expo": {
    "jsEngine": "hermes",
    "name": "hipeac-mobile-app",
    "slug": "hipeac-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ariadne.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ariadne.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "jsEngine": "jsc",
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.hipeacmobileapp"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/ariadne.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.hipeacmobileapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "expo-font",
      "react-native-ble-plx"
    ]
  }
}